{
    "name": "reactphp-x/queue",
    "description": "A queue implementation for ReactPHP using Redis",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "wpjscc",
            "email": "wpjscc@gmail.com"
        }
    ],
    "require": {
        "wpjscc/redis-react": "^3.0",
        "reactphp-x/serializable-closure": "^1.0",
        "reactphp-x/process-manager": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ReactphpX\\Queue\\": "src/"
        }
    },
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
